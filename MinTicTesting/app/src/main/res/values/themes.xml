<resources xmlns:tools="http://schemas.android.com/tools" xmlns:app="http://schemas.android.com/apk/res-auto">
    <!-- Base application theme. -->
    <style name="Theme.MinTicTesting" parent="Theme.Material3.Dark.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/primaryColor</item>
        <item name="colorPrimaryDark">@color/darkColor</item>
        <item name="colorAccent">@color/highLightColor</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="ThemeTextInputLayout">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">@dimen/longSpace</item>
        <item name="android:layout_marginTop">@dimen/middleSpace</item>
        <item name="helperTextEnabled">true</item>
        <item name="android:textColor">@color/primaryColor</item>
        <item name="android:textColorHighlight">@color/highLightColor</item>
        <item name="android:textColorHint">@color/secundaryColor</item>
        <item name="android:textColorLink">@color/darkColor</item>
        <item name="startIconTint">@color/highLightColor</item>
    </style>

    <style name="Widget.App.Button.OutlinedButton.IconOnly" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="iconPadding">0dp</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingRight">12dp</item>
        <item name="android:minWidth">48dp</item>
        <item name="android:minHeight">48dp</item>
    </style>

        <!-- Base application theme. -->
        <style name="AppTheme" parent="Theme.MaterialComponents">
            <!-- Customize your theme here. -->
            <item name="colorPrimary">@color/primaryColor</item>
            <item name="colorPrimaryDark">@color/darkColor</item>
            <item name="colorAccent">@color/highLightColor</item>
        </style>


</resources>